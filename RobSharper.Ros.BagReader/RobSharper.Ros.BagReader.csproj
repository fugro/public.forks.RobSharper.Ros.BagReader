<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Authors>Guido Breitenhuber</Authors>
        <Copyright>JOANNEUM RESEARCH Forschungsgesellschaft mbH</Copyright>
        <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
        <Description>
            Fugro fork of RobSharper.Ros.BagReader can process ROSBAG files.
        </Description>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/fugro/public.forks.RobSharper.Ros.BagReader.git</RepositoryUrl>
        <PackageTags>ROS;RobSharper;RobSharper.Ros;ROSBAG;</PackageTags>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
        <PackageOutputPath>$(MSBuildThisFileDirectory)/../Artifacts</PackageOutputPath>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="RobSharper.Ros.MessageEssentials" Version="1.0.7" />
    </ItemGroup>

</Project>
